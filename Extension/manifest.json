{
  "name": "Bogus Story Meter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "For people who want the truth and nothing but the truth.",
  "icons": { "128": "images/BSMIcon.png" },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["identity", "tabs", "background", "activeTab", "http://localhost:8080/test"],
  "browser_action": {
    "default_title": "Bogus Story Meter",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css http://localhost:8080/test; object-src 'self' ",
  "key": "",
  "oauth2": {
    "client_id": "432670489929-0g6anrb5qg3b0o83m0ef5cdolsbotkps.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login"
    ]
  }
}